generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model study_memo {
  id          Int      @id @default(autoincrement())
  study_date  DateTime @default(now()) @db.Timestamp(0) // 学習の時間
  title       String // タイトル
  description String @db.Text // 学習の内容
  duration    Int // 時間
  created_at  DateTime @default(now()) @db.Timestamp(0)
  updated_at  DateTime @default(now()) @db.Timestamp(0)
}